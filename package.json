  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^6.0.2",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "mongodb": "^3.6.6",
    "mongoose": "^5.12.5",
